{
  "name": "codenexlify-website",
  "version": "2.0.0",
  "description": "CodeNexlify - Yenilikçi Teknoloji Çözümleri Web Sitesi",
  "main": "src/index.js",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint src --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write src/**/*.{js,jsx,ts,tsx,css,scss,html}",
    "analyze": "vite-bundle-analyzer",
    "lighthouse": "lighthouse http://localhost:3000 --output html --output-path ./reports/lighthouse.html",
    "serve": "http-server dist -p 3000"
  },
  "keywords": [
    "web-development",
    "ai-tools",
    "technology-solutions",
    "codenexlify",
    "javascript",
    "pwa"
  ],
  "author": "CodeNexlify Team",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.23.0",
    "@babel/preset-env": "^7.23.0",
    "@eslint/js": "^9.0.0",
    "@types/jest": "^29.5.0",
    "@vitejs/plugin-legacy": "^5.0.0",
    "autoprefixer": "^10.4.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "http-server": "^14.1.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lighthouse": "^11.0.0",
    "postcss": "^8.4.0",
    "prettier": "^3.0.0",
    "sass": "^1.69.0",
    "vite": "^5.0.0",
    "vite-bundle-analyzer": "^0.7.0",
    "vite-plugin-pwa": "^0.17.0",
    "workbox-window": "^7.0.0"
  },
  "dependencies": {
    "fuse.js": "^7.0.0",
    "gsap": "^3.12.0",
    "intersection-observer": "^0.12.0",
    "prismjs": "^1.29.0",
    "swiper": "^11.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ],
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
